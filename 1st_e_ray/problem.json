{
  "$schema": "../schema.json",
  "label": "hpc",
  "submit": {
    "form": {
      "files": [
        {
          "path": ".metadata.json",
          "label": "Metadata",
          "type": {
            "metadata": {
              "items": [
                {
                  "key": "language",
                  "label": "Language",
                  "type": {
                    "select": {
                      "options": [
                        "Python3"
                      ]
                    }
                  }
                }
              ]
            }
          }
        },
        {
          "path": "main.py",
          "type": {
            "editor": {
              "language": "python"
            }
          },
          "label": "Source Code"
        }
      ]
    }
  },
  "judge": {
    "adapter": "hpc",
    "config": {
      "entrance": {
        "script": "judge.sh",
        "command": ""
      },
      "environment": {
        "tags": [],
        "exclude-tags": [],
        "script-limits": {
          "cpu": 200,
          "memory": 4096
        },
        "estimated-resource": {}
      }
    }
  }
}
